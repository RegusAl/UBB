     1                                  ; Se da un sir de caractere S. Sa se construiasca sirul D care sa contina toate literele mici din sirul S.
     2                                  ;    Exemplu:
     3                                  ;    S: 'a', 'A', 'b', 'B', '2', '%', 'x'
     4                                  ;    D: 'a', 'b', 'x'
     5                                  bits 32
     6                                  global start
     7                                  
     8                                  extern exit
     9                                  import exit msvcrt.dll
    10                                  
    11                                  segment data use32 class=data
    12 00000000 6141624232257860            s db 'a', 'A', 'b', 'B', '2', '%', 'x', '`'
    13                                      len  equ ($-s)
    14 00000008 00<rept>                    d times len db 0
    15                                      
    16                                  segment code use32 class=code
    17                                      start:
    18 00000000 B908000000                      mov ecx, len
    19 00000005 E322                            jecxz final
    20 00000007 BE00000000                      mov esi, 0
    21 0000000C BF00000000                      mov edi, 0
    22                                          
    23                                          repeta:
    24 00000011 8A86[00000000]                      mov al, [s+esi] 
    25                                              
    26 00000017 3C60                                cmp al, 'a'-1               ; Compar al cu 
    27 00000019 760B                                jbe nu
    28 0000001B 3C7A                                    cmp al, 'z'
    29 0000001D 7707                                    jnbe nu
    30 0000001F 8887[08000000]                          mov [d+edi], al
    31 00000025 47                                      inc(edi)
    32                                              nu:
    33                                          
    34 00000026 46                                  inc(esi)
    35 00000027 E2E8                            loop repeta
    36                                          
    37                                          
    38                                      final:
    39                                      
    40 00000029 6A00                        push dword 0
    41 0000002B FF15[00000000]              call [exit]
