     1                                  ; Se da un sir S de dublucuvinte.
     2                                  ; Sa se obtina sirul D format din octetii inferiori ai cuvintelor inferioare din elementele sirului de dublucuvinte, care sunt multiplii de 7.
     3                                  ;   Exemplu:
     4                                  ; s DD 12345607h, 1A2B3C15h, 13A33412h
     5                                  ; d DB 07h, 15h
     6                                  bits 32
     7                                  global start
     8                                  
     9                                  extern exit
    10                                  import exit msvcrt.dll
    11                                  
    12                                  segment data use32 class=data
    13 00000000 07563412153C2B1A12-         s dd 12345607h, 1A2B3C15h, 13A33412h
    13 00000009 34A313             
    14                                      len equ ($-s)/4
    15 0000000C 00<rept>                    d times len db 0
    16 0000000F 07                          sapte db 7
    17                                      
    18                                  segment code use32 class=code
    19                                      start:
    20 00000000 B903000000                      mov ecx, len 
    21 00000005 E323                            jecxz final
    22 00000007 FC                              cld
    23 00000008 BE[00000000]                    mov esi, s
    24 0000000D BF[0C000000]                    mov edi, d
    25                                          
    26                                          repeta:
    27 00000012 AC                                  lodsb
    28 00000013 88C3                                mov bl, al
    29 00000015 B400                                mov ah, 0    ; al --> ax
    30 00000017 F635[0F000000]                      div byte [sapte] 
    31 0000001D 80FC00                              cmp ah, 0
    32 00000020 7503                                jne peste
    33 00000022 88D8                                mov al, bl
    34 00000024 AA                                  stosb
    35                                          peste:
    36 00000025 66AD                            lodsw
    37 00000027 AC                              lodsb
    38 00000028 E2E8                            loop repeta
    39                                          
    40                                      final:
    41 0000002A 6A00                        push dword 0
    42 0000002C FF15[00000000]              call [exit]
