     1                                   ; Se da un sir de caractere S. Sa se construiasca sirul D care sa contina toate caracterele cifre din sirul S.
     2                                  ;   Exemplu:
     3                                  ;   S: '+', '4', '2', 'a', '8', '4', 'X', '5'
     4                                  ;   D: '4', '2', '8', '4', '5'
     5                                  bits 32
     6                                  global start
     7                                  
     8                                  extern exit
     9                                  import exit msvcrt.dll
    10                                  
    11                                  segment data use32 class=data
    12 00000000 2B34326138345835            s db '+', '4', '2', 'a', '8', '4', 'X', '5'
    13                                      len equ $-s
    14 00000008 00<rept>                    d times len db 0
    15                                      
    16                                  segment code use32 class=code
    17                                      start:
    18 00000000 B908000000                      mov ecx, len
    19 00000005 E322                            jecxz final
    20 00000007 BE00000000                      mov esi, 0
    21 0000000C BF00000000                      mov edi, 0
    22                                          repeta:
    23 00000011 8A86[00000000]                      mov al, [s+esi]
    24 00000017 3C30                                cmp al, '0'
    25 00000019 720B                                jb peste
    26 0000001B 3C39                                cmp al, '9'
    27 0000001D 7707                                ja peste
    28 0000001F 8887[08000000]                      mov [d+edi], al       
    29 00000025 47                                  inc(edi)
    30                                          peste:
    31 00000026 46                              inc(esi)
    32 00000027 E2E8                            loop repeta
    33                                          
    34                                      final:
    35 00000029 6A00                        push dword 0
    36 0000002B FF15[00000000]              call [exit]
