     1                                  ; 2-(c+d)+(a+b-c) 
     2                                  bits 32
     3                                  
     4                                  global start
     5                                  
     6                                  extern exit
     7                                  import exit msvcrt.dll
     8                                  
     9                                  ; a, b, c, d - byte
    10                                  segment data use32 class=data
    11 00000000 05                          a db 5
    12 00000001 07                          b db 7
    13 00000002 0F                          c db 15
    14 00000003 14                          d db 20
    15                                  
    16                                  segment code use32 class=code
    17                                      start:
    18                                      
    19                                          ; 2 - (c + d)
    20 00000000 B002                            mov AL, 2       ; AL = 2
    21 00000002 8A25[02000000]                  mov AH, [c]    ; AH = c = 15
    22 00000008 0225[03000000]                  add AH, [d]     ; AH = c + d = 35
    23 0000000E 28E0                            sub AL, AH     ; AL = AL - AH = 2 - 35 = -33
    24                                          
    25                                          ; a + b - c
    26 00000010 8A25[00000000]                  mov AH, [a]     ; AH = a = 5
    27 00000016 0225[01000000]                  add AH, [b]      ; AH = a + b = 5 + 7 = 12
    28 0000001C 2A25[02000000]                  sub AH, [c]      ; AH = a + b - c = 12 - 15 = -3
    29                                          
    30                                          ; 2 - (c + d) + (a + b - c)
    31 00000022 00E0                            add AL, AH      ; -33 + (-3) = -36
    32                                          
    33 00000024 6A00                            push dword 0
    34 00000026 FF15[00000000]                  call [exit]
