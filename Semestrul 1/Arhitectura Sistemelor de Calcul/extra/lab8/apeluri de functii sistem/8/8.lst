     1                                  ; Se da un numar natural a (a: dword, definit in segmentul de date). Sa se citeasca un numar natural b si sa se calculeze: a + a/b. Sa se afiseze rezultatul operatiei.
     2                                  ; Valorile vor fi afisate in format decimal (baza 10) cu semn. 
     3                                  bits 32
     4                                  global start
     5                                  
     6                                  extern exit, scanf, printf
     7                                  import exit msvcrt.dll
     8                                  import scanf msvcrt.dll
     9                                  import printf msvcrt.dll
    10                                  
    11                                  segment data use32 class=data
    12 00000000 14000000                    a dd 20
    13 00000004 00000000                    b dd 0
    14 00000008 62203D2000                  citire db "b = ", 0
    15 0000000D 256400                      format db "%d", 0
    16 00000010 52657A756C74617475-         afisare db "Rezultatul este: %d", 0
    16 00000019 6C20657374653A2025-
    16 00000022 6400               
    17 00000024 00000000                    rezultat dd 0
    18                                  
    19                                  segment code use32 class=code
    20                                      start:
    21                                      
    22                                          ; b = 
    23 00000000 68[08000000]                    push dword citire
    24 00000005 FF15[00000000]                  call [printf]
    25 0000000B 83C404                          add esp, 1*4
    26                                   
    27                                          ; citire numar de la tastatura
    28 0000000E 68[04000000]                    push dword b
    29 00000013 68[0D000000]                    push dword format
    30 00000018 FF15[00000000]                  call [scanf]
    31 0000001E 83C408                          add esp, 2*4
    32                                          
    33                                          ; a + a / b
    34 00000021 A1[00000000]                    mov eax, [a]
    35 00000026 99                              cdq
    36 00000027 F73D[04000000]                  idiv dword [b]
    37 0000002D 0305[00000000]                  add eax, [a]
    38 00000033 A3[24000000]                    mov [rezultat], eax
    39                                  
    40                                          ; afisare rezultat
    41 00000038 FF35[24000000]                  push dword [rezultat]
    42 0000003E 68[10000000]                    push dword afisare
    43 00000043 FF15[00000000]                  call [printf]
    44 00000049 83C408                          add esp, 2*4
    45                                      
    46 0000004C 6A00                        push dword 0
    47 0000004E FF15[00000000]              call [exit]
