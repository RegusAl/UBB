     1                                  ; Se citesc de la tastatura doua numere a si b. Sa se calculeze valoarea expresiei (a-b)*k, k fiind o constanta definita in segmentul de date. Afisati valoarea expresiei (in baza 16). 
     2                                  bits 32
     3                                  global start
     4                                  
     5                                  extern exit, printf, scanf
     6                                  import exit msvcrt.dll
     7                                  import scanf msvcrt.dll
     8                                  import printf msvcrt.dll
     9                                  
    10                                  segment data use32 class=data
    11 00000000 00000000                    a dd 0
    12 00000004 00000000                    b dd 0
    13 00000008 02000000                    k dd 2
    14 0000000C 61203D2000                  mesaj_a db "a = ", 0
    15 00000011 62203D2000                  mesaj_b db "b = ", 0
    16 00000016 256400                      format_dec db "%d", 0
    17 00000019 72657A756C7461743A-         afisare db "rezultat: %xh"
    17 00000022 20257868           
    18                                  
    19                                  segment code use32 class=code
    20                                      start:
    21                                          
    22                                          ; a =
    23 00000000 68[0C000000]                    push dword mesaj_a
    24 00000005 FF15[00000000]                  call [printf]
    25 0000000B 83C404                          add esp, 1*4
    26                                          
    27                                          ; citim a
    28 0000000E 68[00000000]                    push dword a
    29 00000013 68[16000000]                    push dword format_dec
    30 00000018 FF15[00000000]                  call [scanf]
    31 0000001E 83C408                          add esp, 2*4
    32                                          
    33                                          ; b =
    34 00000021 68[11000000]                    push dword mesaj_b
    35 00000026 FF15[00000000]                  call [printf]
    36 0000002C 83C404                          add esp, 1*4
    37                                          
    38                                          ; citim b
    39 0000002F 68[04000000]                    push dword b
    40 00000034 68[16000000]                    push dword format_dec
    41 00000039 FF15[00000000]                  call [scanf]
    42 0000003F 83C408                          add esp, 2*4
    43                                          
    44                                          ; edx:eax = (a - b) * k
    45 00000042 A1[00000000]                    mov eax, [a]
    46 00000047 2B05[04000000]                  sub eax, [b]
    47 0000004D F72D[08000000]                  imul dword [k]
    48                                          
    49                                          ; afisare
    50 00000053 52                              push edx
    51 00000054 50                              push eax
    52 00000055 68[19000000]                    push dword afisare
    53 0000005A FF15[00000000]                  call [printf]
    54 00000060 83C40C                          add esp, 3*4
    55                                          
    56 00000063 6A00                        push dword 0
    57 00000065 FF15[00000000]              call [exit]
