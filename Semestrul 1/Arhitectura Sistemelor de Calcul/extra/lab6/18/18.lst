     1                                  ; Se da un sir de dublucuvinte. Sa se ordoneze crescator sirul cuvintelor superioare ale acestor dublucuvinte. Cuvintele inferioare raman neschimbate.
     2                                  ;   Exemplu:
     3                                  ;       sir DD 12AB5678h, 1256ABCDh, 12344344h 
     4                                  ;   rezultatul
     5                                  ;       12345678h, 1256ABCDh, 12AB4344h.
     6                                  bits 32
     7                                  global start
     8                                  
     9                                  extern exit
    10                                  import exit msvcrt.dll
    11                                  
    12                                  segment data use32 class=data
    13 00000000 7856AB12CDAB561244-         sir dd 12AB5678h, 1256ABCDh, 12344344h 
    13 00000009 433412             
    14                                      len equ ($-sir)/4
    15 0000000C 0000<rept>                  cuvinte_superioare times len dw 0
    16 00000012 00                          over db 0
    17                                      
    18                                  
    19                                  segment code use32 class=code
    20                                      start:
    21                                          
    22 00000000 B903000000                      mov ecx, len
    23 00000005 E368                            jecxz final
    24 00000007 BE[00000000]                    mov esi, sir
    25 0000000C BF[0C000000]                    mov edi, cuvinte_superioare
    26 00000011 FC                              cld
    27                                          repeta:
    28 00000012 66AD                                lodsw
    29 00000014 66A5                                movsw
    30 00000016 E2FA                            loop repeta
    31                                          
    32                                          while_loop:
    33                                          
    34 00000018 B902000000                          mov ecx, len-1
    35 0000001D E331                                jecxz final_loop
    36                                              
    37 0000001F BE[0C000000]                        mov esi, cuvinte_superioare
    38 00000024 BF[0E000000]                        mov edi, cuvinte_superioare+2
    39 00000029 FC                                  cld
    40 0000002A C605[12000000]00                    mov byte [over], 0
    41                                              
    42                                              for_loop:
    43 00000031 66A7                                    cmpsw
    44 00000033 7219                                    jb final_if
    45 00000035 83EE02                                      sub esi, 2
    46 00000038 83EF02                                      sub edi, 2
    47 0000003B 66AD                                        lodsw
    48 0000003D 83EF02                                      sub edi, 2
    49 00000040 66A5                                        movsw
    50 00000042 83EE02                                      sub esi, 2
    51 00000045 66AB                                        stosw
    52 00000047 C605[12000000]01                            mov byte [over], 1
    53                                              
    54                                                  final_if:
    55                                                  
    56 0000004E E2E1                                loop for_loop
    57                                              
    58                                              final_loop:
    59 00000050 803D[12000000]00                    cmp byte [over], 0
    60 00000057 75BF                                jne while_loop
    61                                              
    62                                          ; concatenare
    63 00000059 B903000000                      mov ecx, len
    64 0000005E BE[0C000000]                    mov esi, cuvinte_superioare
    65 00000063 BF[00000000]                    mov edi, sir
    66                                          concatenare:
    67 00000068 83C702                              add edi, 2
    68 0000006B 66A5                                movsw
    69 0000006D E2F9                            loop concatenare
    70                                      final:
    71 0000006F 6A00                        push dword 0
    72 00000071 FF15[00000000]              call [exit]
