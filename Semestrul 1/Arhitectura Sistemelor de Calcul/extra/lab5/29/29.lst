     1                                   ; Se da un sir de octeti S. Sa se construiasca sirul D ale carui elemente reprezinta suma fiecaror doi octeti consecutivi din sirul S.
     2                                  ;   Exemplu:
     3                                  ;   S: 1, 2, 3, 4, 5, 6
     4                                  ;   D: 3, 5, 7, 9, 11
     5                                  bits 32
     6                                  global start
     7                                  
     8                                  extern exit
     9                                  import exit msvcrt.dll
    10                                  
    11                                  segment data use32 class=data
    12 00000000 010203040506                s db 1, 2, 3, 4, 5, 6
    13                                      len equ $-s
    14 00000006 00<rept>                    d times len-1 db 0
    15                                     
    16                                  segment code use32 class=code
    17                                      start:
    18                                      
    19 00000000 B905000000                      mov ecx, len-1
    20 00000005 E31D                            jecxz final
    21 00000007 BE00000000                      mov esi, 0
    22                                          
    23                                          repeta:
    24 0000000C 8A86[00000000]                      mov al, [s+esi]
    25 00000012 46                                  inc(esi)
    26 00000013 8A9E[00000000]                      mov bl, [s+esi]
    27 00000019 00D8                                add al, bl
    28 0000001B 8887[06000000]                      mov [d+edi], al
    29 00000021 47                                  inc(edi)
    30 00000022 E2E8                            loop repeta
    31                                         
    32                                      final:
    33                                      
    34 00000024 6A00                        push dword 0
    35 00000026 FF15[00000000]              call [exit]
