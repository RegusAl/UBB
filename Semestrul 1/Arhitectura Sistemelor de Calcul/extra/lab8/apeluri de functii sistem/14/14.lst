    10                                  segment data use32 class=data
    11 00000000 00000000                    a dd 0
    12 00000004 00000000                    b dd 0
    13 00000008 61203D2000                  mesaj_a db "a = ", 0
    14 0000000D 62203D2000                  mesaj_b db "b = ", 0
    15 00000012 2578                        format_hexa db "%x"
    16 00000014 52657A756C74617475-         afisare db "Rezultatul este %d"
    16 0000001D 6C2065737465202564 
    17                                  
    18                                  segment code use32 class=code
    19                                      start:
    20                                      
    21                                          ; a = 
    22 00000000 68[08000000]                    push dword mesaj_a
    23 00000005 FF15[00000000]                  call [printf]
    24 0000000B 83C404                          add esp, 1*4
    25                                          
    26                                          ; citire a
    27 0000000E 68[00000000]                    push dword a
    28 00000013 68[12000000]                    push dword format_hexa
    29 00000018 FF15[00000000]                  call [scanf]
    30 0000001E 83C408                          add esp, 2*4
    31                                          
    32                                          ; b = 
    33 00000021 68[0D000000]                    push dword mesaj_b
    34 00000026 FF15[00000000]                  call [printf]
    35 0000002C 83C404                          add esp, 1*4
    36                                          
    37                                          ; citire b
    38 0000002F 68[04000000]                    push dword b
    39 00000034 68[12000000]                    push dword format_hexa
    40 00000039 FF15[00000000]                  call [scanf]
    41 0000003F 83C408                          add esp, 2*4
    42                                          
    43                                          ; a + b
    44 00000042 A1[00000000]                    mov eax, [a]
    45 00000047 0305[04000000]                  add eax, [b]
    46                                          
    47 0000004D 50                              push eax
    48 0000004E 68[14000000]                    push dword afisare
    49 00000053 FF15[00000000]                  call [printf]
