     1                                  ; Se citesc de la tastatura doua numere a si b. Sa se calculeze valoarea expresiei (a/b)*k, k fiind o constanta definita in segmentul de date. Afisati valoarea expresiei (in baza 2). 
     2                                  bits 32
     3                                  global start
     4                                  
     5                                  extern exit, scanf, printf
     6                                  import exit msvcrt.dll
     7                                  import scanf msvcrt.dll
     8                                  import printf msvcrt.dll
     9                                  
    10                                  segment data use32 class=data
    11 00000000 00000000                    a dd 0
    12 00000004 00000000                    b dd 0
    13 00000008 02000000                    k dd 2
    14 0000000C 61203D2000                  mesaj_a db "a = ", 0
    15 00000011 62203D2000                  mesaj_b db "b = ", 0
    16 00000016 256400                      format_dec db "%d", 0
    17 00000019 257300                      format_str db "%s", 0
    18 0000001C 00<rept>                    bit times 32 db 0
    19                                  
    20                                  segment code use32 class=code
    21                                      start:
    22                                      
    23                                          ; a = 
    24 00000000 68[0C000000]                    push dword mesaj_a
    25 00000005 FF15[00000000]                  call [printf]
    26 0000000B 83C404                          add esp, 1*4
    27                                          
    28                                          ; citim a
    29 0000000E 68[00000000]                    push dword a
    30 00000013 68[16000000]                    push dword format_dec
    31 00000018 FF15[00000000]                  call [scanf]
    32 0000001E 83C408                          add esp, 2*4
    33                                          
    34                                          ; b = 
    35 00000021 68[11000000]                    push dword mesaj_b
    36 00000026 FF15[00000000]                  call [printf]
    37 0000002C 83C404                          add esp, 1*4
    38                                          
    39                                          ; citim b
    40 0000002F 68[04000000]                    push dword b
    41 00000034 68[16000000]                    push dword format_dec
    42 00000039 FF15[00000000]                  call [scanf]
    43 0000003F 83C408                          add esp, 2*4
    44                                          
    45                                          ; edx:eax = (a / b) * k
    46 00000042 A1[00000000]                    mov eax, [a]
    47 00000047 99                              cdq
    48 00000048 F73D[04000000]                  idiv dword [b]
    49 0000004E F725[08000000]                  mul dword [k]
    50                                          
    51 00000054 B920000000                      mov ecx, 32
    52 00000059 89C3                            mov ebx, eax
    53 0000005B BF[1C000000]                    mov edi, bit
    54                                          
    55                                          repeta:
    56 00000060 D1E3                                shl ebx, 1
    57 00000062 B000                                mov al, 0
    58 00000064 1430                                adc al, '0'
    59 00000066 AA                                  stosb
    60 00000067 E2F7                            loop repeta
    61                                          
    62                                          ; afisare
    63 00000069 68[1C000000]                    push bit  
    64 0000006E 68[19000000]                    push dword format_str
    65 00000073 FF15[00000000]                  call [printf]
    66 00000079 83C408                          add esp, 2*4  
    67                                          
    68 0000007C 6A00                        push dword 0
    69 0000007E FF15[00000000]              call [exit]
