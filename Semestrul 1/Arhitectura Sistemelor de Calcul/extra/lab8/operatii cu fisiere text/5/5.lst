    44 00000035 FF35[19000000]                  push dword [descriptor_fisier]
    45 0000003B FF15[00000000]                  call [fclose]
    46 00000041 83C404                          add esp, 1*4
    47                                          
    48                                          ; numarare caractere speciale
    49 00000044 BE00000000                      mov esi, 0
    50 00000049 FC                              cld
    51 0000004A BB00000000                      mov ebx, 0
    52                                          repeta:
    53                                          
    54 0000004F BF00000000                          mov edi, 0
    55 00000054 B91F000000                          mov ecx, 31
    56                                              caractere_speciale_loop:
    57 00000059 8A97[81000000]                          mov dl, [caractere_speciale+edi]
    58 0000005F 47                                      inc(edi)
    59 00000060 3A96[1D000000]                          cmp dl, [text+esi]
    60 00000066 7404                                    je numarare
    61                                                  
