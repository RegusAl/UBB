     1                                  ; Se dau 2 siruri de octeti A si B. Sa se construiasca sirul R care sa contina elementele lui B in ordine inversa urmate de elementele negative ale lui A.
     2                                      
     3                                      ; Exemplu:
     4                                  
     5                                      ;   A: 2, 1, -3, 3, -4, 2, 6
     6                                      ;   B: 4, 5, 7, 6, 2, 1
     7                                      ;   R: 1, 2, 6, 7, 5, 4, -3, -4
     8                                  
     9                                  bits 32
    10                                  
    11                                  global start
    12                                  
    13                                  extern exit
    14                                  import exit msvcrt.dll
    15                                  
    16                                  segment data use32 class=data
    17 00000000 0201FD03FC0206              a db 2, 1, -3, 3, -4, 2, 6
    18                                      len_a equ ($-a)
    19 00000007 040507060201                b db 4, 5, 7, 6, 2, 1
    20                                      len_b equ ($-b)
    21 0000000D 00<rept>                    r times len_b+len_a db 0
    22                                  
    23                                  segment code use32 class=code
    24                                      start:
    25                                      
    26 00000000 B906000000                      mov ecx, len_b
    27 00000005 BE00000000                      mov esi, 0
    28 0000000A E334                            jecxz sfarsit
    29                                          ; loop pentru a lua sirul B in ordine inversa 
    30                                          repetaB:
    31 0000000C 8A86[07000000]                      mov al, [b+esi]
    32 00000012 8881[0C000000]                      mov [r+ecx-1], al
    33 00000018 46                                  inc esi
    34 00000019 E2F1                            loop repetaB
    35                                          
    36 0000001B B907000000                      mov ecx, len_a
    37 00000020 BF00000000                      mov edi, 0
    38 00000025 BE00000000                      mov esi, 0
    39 0000002A E314                            jecxz sfarsit
    40                                          ; loop pentru a lua numerele negative din sirul A
    41                                          repetaA:
    42 0000002C 8A86[00000000]                      mov al, [a+esi]
    43 00000032 46                                  inc esi
    44 00000033 A8F2                                test al, 0F2h ; testam daca SingFlag este 1 sau 0
    45 00000035 7907                                jns pozitiv    ; jump la eticheta pozitiv daca SF este 0
    46 00000037 8887[13000000]                          mov [r+len_b+edi], al
    47 0000003D 47                                      inc edi
    48                                              pozitiv:  
    49 0000003E E2EC                            loop repetaA
    50                                              
    51                                          sfarsit:
    52                                      
    53 00000040 6A00                            push dword 0
    54 00000042 FF15[00000000]                  call [exit]
