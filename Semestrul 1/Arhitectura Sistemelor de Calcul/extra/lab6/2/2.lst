     1                                  ; Se da un sir de cuvinte.
     2                                  ; Sa se obtina din acesta un sir de dublucuvinte, in care fiecare dublucuvant
     3                                  ; va contine nibble-urile despachetate pe octet (fiecare cifra hexa va fi precedata de un 0), aranjate crescator in interiorul dublucuvantului.
     4                                  ; Exemplu:
     5                                  ; pentru sirul initial:
     6                                  ; 1432h, 8675h, 0ADBCh, ...
     7                                  ; Se va obtine:
     8                                  ; 01020304h,  05060708h, 0A0B0C0Dh, ... 
     9                                  bits 32
    10                                  global start
    11                                  
    12                                  extern exit
    13                                  import exit msvcrt.dll
    14                                  
    15                                  
    16                                  segment data use32 class=data
    17 00000000 32147586BCAD            	s dw 1432h, 8675h, 0ADBCh
    18                                      len equ ($-s)/2
    19 00000006 00<rept>                    octeti times 4 db 0
    20 0000000A 00000000<rept>              d times len dd 0
    21                                      
    22                                  segment code use32 class=code
    23                                      start:
    24 00000000 B903000000                      mov ecx, len
    25 00000005 E32C                            jecxz final
    26 00000007 FC                              cld
    27 00000008 BF[0A000000]                    mov edi, d
    28 0000000D BE00000000                      mov esi,0
    29                                          
    30                                      parcurg_sirul:
    31                                          
    32 00000012 51                              push ecx
    33 00000013 57                              push edi
    34                                          
    35                                          ; 1. obtin sirul de octeti
    36 00000014 B902000000                      mov ecx, 2
    37                                          ;mov edi, octeti
    38                                          formez_octeti:
    39 00000019 8A86[00000000]                      mov al, [s+esi]      ; AL = 32h
    40 0000001F 88C2                                mov dl, al           ; DL = 32h
    41 00000021 240F                                and al, 0000_1111b   ; AL = 02h
    42 00000023 80E2F0                              and dl, 1111_0000b
    43 00000026 C0EA04                              shr dl, 4           ; DL = 03h
    44 00000029 AA                                  stosb
    45 0000002A 88D0                                mov al, dl
    46 0000002C AA                                  stosb
    47 0000002D 46                                  inc esi
    48 0000002E E2E9                            loop formez_octeti
    49                                   
    50 00000030 59                              pop ecx
    51 00000031 E2DF                        loop parcurg_sirul
    52                                          
    53                                          final:
    54 00000033 6A00                        push dword 0
    55 00000035 FF15[00000000]              call [exit]
