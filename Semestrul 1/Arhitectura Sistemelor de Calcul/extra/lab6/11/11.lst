     1                                  ; Se da un sir A de dublucuvinte. Construiti doua siruri de octeti  
     2                                  ;  - B1: contine ca elemente partea superioara a cuvintelor superioare din A
     3                                  ;  - B2: contine ca elemente partea inferioara a cuvintelor inferioare din A
     4                                  bits 32
     5                                  global start
     6                                  
     7                                  extern exit
     8                                  import exit msvcrt.dll
     9                                  
    10                                  segment data use32 class=data
    11 00000000 785634128877665514-         A dd 12345678h, 55667788h, 90123214h, 11223344h
    11 00000009 32129044332211     
    12                                      len equ ($-A)/4
    13 00000010 00<rept>                    B1 times len db 0
    14 00000014 00<rept>                    B2 times len db 0
    15                                  
    16                                  segment code use32 class=code
    17                                      start:
    18 00000000 B904000000                      mov ecx, len
    19 00000005 E31E                            jecxz final
    20                                          
    21 00000007 FC                              cld
    22 00000008 BE[00000000]                    mov esi, A
    23 0000000D BF00000000                      mov edi, 0
    24                                          
    25                                          repeta:
    26 00000012 AC                                  lodsb        ; AL = <DS:ESI>
    27 00000013 8887[14000000]                      mov [B2+edi], al
    28                                              
    29 00000019 66AD                                lodsw
    30                                              
    31 0000001B AC                                  lodsb
    32 0000001C 8887[10000000]                      mov [B1+edi], al
    33                                  
    34 00000022 47                                  inc(edi)
    35 00000023 E2ED                            loop repeta
    36                                            
    37                                      final:
    38 00000025 6A00                        push dword 0
    39 00000027 FF15[00000000]              call [exit]
